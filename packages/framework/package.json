{
  "name": "@mcsb/framework",
  "version": "1.0.0-pre.2",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "license": "MIT",
  "scripts": {
    "lint": "tsc && eslint . && madge --circular src",
    "test": "globstar -- node --inspect --experimental-specifier-resolution=node --no-warnings=ExperimentalWarning --loader ts-node/esm --test \"src/**/*.test.ts\"",
    "prebuild": "rimraf dist",
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "postbuild": "rimraf -g dist/rollup.config.* dist/src",
    "prepublishOnly": "lerna run build --scope @mcsb/framework"
  },
  "dependencies": {
    "@mcsb/api": "*",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "eslint-config-custom": "^1.0.0-pre.2",
    "rollup": "^4.1.4",
    "tsconfig": "^1.0.0-pre.2",
    "typescript": "^5.0.0"
  },
  "watcherOptions": {
    "files": [
      "src",
      "index.ts",
      "rollup.config.ts",
      "package.json",
      "tsconfig.json"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}

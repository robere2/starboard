{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://starboard.ecr.dev/schemas/hypixel/HypixelPlayerResponse.json",
  "title": "HypixelPlayerResponse",
  "description": "Schema for data returned from https://api.hypixel.net/player",
  "oneOf": [{
    "type": "object",
    "properties": {
      "success": {
        "const": true
      },
      "player": {
        "oneOf": [{
          "$ref": "#/definitions/HypixelPlayer"
        }, {
          "type": "null"
        }]
      }
    },
    "required": ["success", "player"]
  }, {
    "type": "object",
    "properties": {
      "success": {
        "const": false
      },
      "cause": {
        "type": "string"
      },
      "throttle": {
        "type": "boolean"
      },
      "global": {
        "type": "boolean"
      }
    },
    "required": ["success", "cause"]
  }],
  "definitions": {
    "HypixelPlayer": {
      "type": ["object"],
      "properties": {
        "_id": {
          "type": "string",
          "pattern": "^[0-9a-f]{24}$"
        },
        "test": {
          "type": "array",
          "items": {
            "type": "integer"
          }
        }
      },
      "required": ["_id"]
    }
  }
}

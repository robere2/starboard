{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://starboard.ecr.dev/schemas/hypixel/guild.json",
  "title": "HypixelGuildResponse",
  "description": "Schema for data returned from https://api.hypixel.net/guild",
  "oneOf": [
    {
      "type": "object",
      "properties": {
        "success": {
          "const": true
        },
        "guild": {
          "oneOf": [
            {
              "$ref": "#/definitions/HypixelGuild"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "required": [
        "success",
        "guild"
      ]
    },
    {
      "type": "object",
      "properties": {
        "success": {
          "const": false
        },
        "cause": {
          "type": "string"
        },
        "throttle": {
          "type": "boolean"
        },
        "global": {
          "type": "boolean"
        }
      },
      "required": [
        "success",
        "cause"
      ]
    }
  ],
  "definitions": {
    "HypixelGuild": {
      "properties": {
        "_id": {
          "pattern": "^[0-9a-f]{24}$",
          "type": "string"
        },
        "achievements": {
          "properties": {
            "EXPERIENCE_KINGS": {
              "type": "integer"
            },
            "ONLINE_PLAYERS": {
              "type": "integer"
            },
            "WINNERS": {
              "type": "integer"
            }
          },
          "type": "object"
        },
        "banner": {
          "properties": {
            "Base": {
              "oneOf": [
                {
                  "pattern": "\\d+",
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "Patterns": {
              "items": {
                "properties": {
                  "Color": {
                    "oneOf": [
                      {
                        "pattern": "\\d+",
                        "type": "string"
                      },
                      {
                        "type": "integer"
                      }
                    ]
                  },
                  "Pattern": {
                    "type": "string"
                  }
                },
                "type": "object"
              },
              "type": "array"
            }
          },
          "type": "object"
        },
        "chatMute": {
          "type": "integer"
        },
        "coins": {
          "type": "integer"
        },
        "coinsEver": {
          "type": "integer"
        },
        "created": {
          "type": "integer"
        },
        "description": {
          "type": "string"
        },
        "exp": {
          "type": "integer"
        },
        "guildExpByGameType": {
          "properties": {
            "ARCADE": {
              "type": "integer"
            },
            "ARENA": {
              "type": "integer"
            },
            "BATTLEGROUND": {
              "type": "integer"
            },
            "BEDWARS": {
              "type": "integer"
            },
            "BUILD_BATTLE": {
              "type": "integer"
            },
            "DUELS": {
              "type": "integer"
            },
            "GINGERBREAD": {
              "type": "integer"
            },
            "HOUSING": {
              "type": "integer"
            },
            "LEGACY": {
              "type": "integer"
            },
            "MCGO": {
              "type": "integer"
            },
            "MURDER_MYSTERY": {
              "type": "integer"
            },
            "PAINTBALL": {
              "type": "integer"
            },
            "PIT": {
              "type": "integer"
            },
            "PROTOTYPE": {
              "type": "integer"
            },
            "QUAKECRAFT": {
              "type": "integer"
            },
            "REPLAY": {
              "type": "integer"
            },
            "SKYBLOCK": {
              "type": "integer"
            },
            "SKYWARS": {
              "type": "integer"
            },
            "SMP": {
              "type": "integer"
            },
            "SPEED_UHC": {
              "type": "integer"
            },
            "SUPER_SMASH": {
              "type": "integer"
            },
            "SURVIVAL_GAMES": {
              "type": "integer"
            },
            "TNTGAMES": {
              "type": "integer"
            },
            "UHC": {
              "type": "integer"
            },
            "VAMPIREZ": {
              "type": "integer"
            },
            "WALLS": {
              "type": "integer"
            },
            "WALLS3": {
              "type": "integer"
            },
            "WOOL_GAMES": {
              "type": "integer"
            }
          },
          "type": "object"
        },
        "joinable": {
          "type": "boolean"
        },
        "legacyRanking": {
          "type": "integer"
        },
        "members": {
          "items": {
            "properties": {
              "expHistory": {
                "patternProperties": {
                  "\\d{4}-\\d{2}-\\d{2}": {
                    "type": "integer"
                  }
                },
                "type": "object"
              },
              "joined": {
                "type": "integer"
              },
              "mutedTill": {
                "type": "integer"
              },
              "name": {
                "type": "string"
              },
              "questParticipation": {
                "type": "integer"
              },
              "rank": {
                "type": "string"
              },
              "uuid": {
                "type": "string"
              }
            },
            "type": "object"
          },
          "type": "array"
        },
        "name": {
          "type": "string"
        },
        "name_lower": {
          "type": "string"
        },
        "preferredGames": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "publiclyListed": {
          "type": "boolean"
        },
        "ranks": {
          "items": {
            "properties": {
              "created": {
                "type": "integer"
              },
              "default": {
                "type": "boolean"
              },
              "name": {
                "type": "string"
              },
              "priority": {
                "type": "integer"
              },
              "tag": {
                "oneOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "null"
                  }
                ]
              }
            },
            "type": "object"
          },
          "type": "array"
        },
        "tag": {
          "type": "string"
        },
        "tagColor": {
          "type": "string"
        }
      },
      "required": [
        "_id",
        "name"
      ],
      "type": "object"
    }
  }
}

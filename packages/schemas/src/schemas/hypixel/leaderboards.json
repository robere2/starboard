{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://starboard.ecr.dev/schemas/hypixel/leaderboards.json",
  "title": "HypixelLeaderboardsResponse",
  "description": "Schema for data returned from https://api.hypixel.net/leaderboards",
  "oneOf": [
    {
      "type": "object",
      "properties": {
        "success": {
          "const": true
        },
        "leaderboards": {
          "type": "object",
          "additionalProperties": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/HypixelLeaderboard"
            }
          }
        }
      },
      "required": [
        "success",
        "leaderboards"
      ]
    },
    {
      "type": "object",
      "properties": {
        "success": {
          "const": false
        },
        "cause": {
          "type": "string"
        },
        "throttle": {
          "type": "boolean"
        },
        "global": {
          "type": "boolean"
        }
      },
      "required": [
        "success",
        "cause"
      ]
    }
  ],
  "definitions": {
    "HypixelLeaderboard": {
      "properties": {
        "count": {
          "type": "integer"
        },
        "leaders": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "location": {
          "type": "string"
        },
        "path": {
          "type": "string"
        },
        "prefix": {
          "type": "string"
        },
        "title": {
          "type": "string"
        }
      },
      "required": [],
      "type": "object"
    }
  }
}
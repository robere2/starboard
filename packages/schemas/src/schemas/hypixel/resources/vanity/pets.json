{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://starboard.ecr.dev/schemas/hypixel/resources/vanity/pets.json",
  "title": "HypixelPetsResponse",
  "description": "Schema for data returned from https://api.hypixel.net/resources/vanity/pets",
  "type": "object",
  "properties": {
    "success": {
      "const": true
    },
    "lastUpdated": {
      "type": "integer"
    },
    "types": {
      "items": {
        "$ref": "#/definitions/HypixelPet"
      },
      "type": "array"
    },
    "rarities": {
      "items": {
        "$ref": "#/definitions/HypixelPetRarity"
      },
      "type": "array"
    }
  },
  "required": [
    "success",
    "types",
    "rarities"
  ],
  "definitions": {
    "HypixelPet": {
      "properties": {
        "key": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "package": {
          "type": "string"
        },
        "rarity": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "type": "object"
    },
    "HypixelPetRarity": {
      "properties": {
        "color": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      },
      "type": "object"
    }
  }
}

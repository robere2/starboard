{
  "$schema": "node_modules/nx/schemas/nx-schema.json",
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "cacheableOperations": [
          "build", "lint", "test"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["{workspaceRoot}/src/**/*.ts", "{workspaceRoot}/package.json"],
      "outputs": ["{workspaceRoot}/dist"]
    }
  }
}
